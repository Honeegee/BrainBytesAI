{
  "name": "brainbytes-frontend",
  "version": "1.0.0",
  "description": "Frontend for BrainBytes",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:quick": "jest --passWithNoTests --silent",
    "test:e2e": "jest --config jest.config.js",
    "test:e2e:debug": "HEADLESS=false jest --config jest.config.js",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test:watch": "jest --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true npm run build"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "chart.js": "^4.4.9",
    "lucide-react": "^0.509.0",
    "next": "^15.3.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^12.0.2",
    "tailwindcss": "^3.3.6",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.14",
    "@types/puppeteer": "^5.4.7",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-puppeteer": "^11.0.0",
    "jest-junit": "^16.0.0",
    "jest-puppeteer": "^11.0.0",
    "prettier": "^3.1.0",
    "puppeteer": "^24.9.0"
  }
}
