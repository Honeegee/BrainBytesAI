{
  "name": "ai-service",
  "version": "1.0.0",
  "description": "AI service for BrainBytes",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:quick": "jest --passWithNoTests --silent",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext .js,.ts --fix",
